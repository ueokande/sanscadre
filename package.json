{
  "name": "Sanscadre",
  "version": "0.2.0",
  "description": "Show your picture on a frameless window",
  "main": "main.js",
  "scripts": {
    "dev:webpack": "webpack --watch --mode development",
    "dev:electron": "NODE_ENV=development nodemon --watch dist/main --exec electron --trace-warnings dist/main/main.js",
    "build": "webpack --mode production",
    "package": "electron-builder --publish never",
    "lint": "eslint --ext '.js,.jsx,.ts,.tsx' src",
    "lint:fix": "eslint --fix --ext '.js,.jsx,.ts,.tsx' src",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ueokande/sanscadre.git"
  },
  "author": "Shin'ya Ueoka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ueokande/sanscadre/issues"
  },
  "homepage": "https://github.com/ueokande/sanscadre#readme",
  "devDependencies": {
    "@babel/preset-react": "^7.10.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@types/jest": "^27.0.2",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.1",
    "@types/uuid": "^8.3.1",
